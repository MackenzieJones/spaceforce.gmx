        draw_set_halign(fa_center);
        draw_text(xCenter, yBottom - scroll + i*50 + offset, credit[i, 0]);
        
    }else{
        draw_set_halign(fa_right);
        draw_text(xCenter - 15, yBottom - scroll + i*50 + offset, credit[i, 0]);
        draw_set_halign(fa_left);
        draw_text(xCenter + 15, yBottom - scroll + i*50 + offset, credit[i, 1]);
    }
}
offset += 100
